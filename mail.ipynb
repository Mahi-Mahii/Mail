    "import imaplib\n",
    "import email\n",
    "import yaml\n",
    "from datetime import datetime\n",
    "\n",
    "# Function to summarize email content using a BART model from transformers\n",
    "def summarize_email_content(content):\n",
    "    from transformers import pipeline\n",
    "    summarizer = pipeline(\"summarization\", model=\"facebook/bart-large-cnn\")\n",
    "    summary = summarizer(content, max_length=150, min_length=30, do_sample=False)\n",
    "    return summary[0]['summary_text']\n",
    "\n",
    "# Load credentials from YAML file\n",
    "with open(\"password.yaml\") as f:\n",
    "    content = f.read()\n",
    "\n",
    "my_passwords = yaml.safe_load(content)\n",
    "username, password = my_passwords[\"username\"], my_passwords[\"password\"]\n",
    "\n",
    "# IMAP server configuration\n",
    "imap_url = 'imap.gmail.com'\n",
    "my_mail = imaplib.IMAP4_SSL(imap_url)\n",
    "\n",
    "try:\n",
    "    # Log in using your credentials\n",
    "    my_mail.login(username, password)\n",
    "\n",
    "    # Select the inbox to fetch messages\n",
    "    my_mail.select('inbox')\n",
    "\n",
    "    # Calculate today's date\n",
    "    today_date = datetime.now().strftime(\"%d-%b-%Y\")\n",
    "\n",
    "    # Search for emails from today\n",
    "    status, data = my_mail.search(None, f'ON {today_date}')\n",
    "\n",
    "    if status != 'OK':\n",
    "        print(\"No messages found!\")\n",
    "        exit()\n",
    "\n",
    "    mail_ids = data[0].split()  # IDs of all emails that we want to fetch\n",
    "\n",
    "    emails = []  # List to store parsed emails\n",
    "\n",
    "    # Iterate through messages and extract data into the emails list\n",
    "    for num in mail_ids:\n",
    "        try:\n",
    "            typ, data = my_mail.fetch(num, '(RFC822)')\n",
    "            for response_part in data:\n",
    "                if isinstance(response_part, tuple):\n",
    "                    msg = email.message_from_bytes(response_part[1])\n",
    "                    email_data = {\n",
    "                        'date': msg['date'],\n",
    "                        'subject': msg['subject'],\n",
    "                        'from': msg['from'],\n",
    "                        'body': ''\n",
    "                    }\n",
    "                    # Extract email body\n",
    "                    for part in msg.walk():\n",
    "                        if part.get_content_type() == 'text/plain':\n",
    "                            try:\n",
    "                                email_data['body'] = part.get_payload(decode=True).decode()\n",
    "                            except UnicodeDecodeError:\n",
    "                                email_data['body'] = part.get_payload(decode=True).decode('latin-1')\n",
    "                    emails.append(email_data)\n",
    "        except imaplib.IMAP4.abort as e:\n",
    "            print(f\"Failed to fetch email {num}: {str(e)}\")\n",
    "            continue\n",
    "\n",
    "    # Now we have all messages, let's summarize each email\n",
    "    email_results = \"\"\n",
    "    for email_data in emails[::-1]:  # Reverse order to show latest email first\n",
    "        email_results += f\"Date: {email_data['date']}\\n\"\n",
    "        email_results += \"=========================================================================\\n\"\n",
    "        email_results += f\"Subject: {email_data['subject']}\\n\"\n",
    "        email_results += f\"From: {email_data['from']}\\n\"\n",
    "        email_results += \"Body:\\n\"\n",
    "        email_results += email_data['body'] + \"\\n\"\n",
    "        email_results += \"----------------------------------------------------\\n\"\n",
    "\n",
    "        # Optionally summarize the email body\n",
    "        summarized_body = summarize_email_content(email_data['body'])\n",
    "        email_results += \"Summary:\\n\" + summarized_body + \"\\n\"\n",
    "        email_results += \"----------------------------------------------------\\n\"\n",
    "\n",
    "    # Print or use the email_results string as needed\n",
    "    print(email_results)\n",
    "\n",
    "except Exception as e:\n",
    "    print(f\"An error occurred: {str(e)}\")\n",
    "finally:\n",
    "    # Logout from the server\n",
    "    my_mail.logout()\n"
   ]
