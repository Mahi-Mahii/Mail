    "import requests\n",
    "import time\n",
    "\n",
    "# Replace with your Hugging Face API URL and token\n",
    "api_url = \"https://api-inference.huggingface.co/models/sshleifer/distilbart-cnn-12-6\"\n",
    "api_token = \"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\"\n",
    "\n",
    "headers = {\n",
    "    \"Authorization\": f\"Bearer {api_token}\"\n",
    "}\n",
    "\n",
    "def summarize_text(text, retries=3, wait=5):\n",
    "    for attempt in range(retries):\n",
    "        try:\n",
    "            response = requests.post(api_url, headers=headers, json={\"inputs\": text})\n",
    "            response.raise_for_status()  # Raise an error for bad status codes\n",
    "            response_data = response.json()\n",
    "            if isinstance(response_data, list) and len(response_data) > 0 and 'summary_text' in response_data[0]:\n",
    "                return response_data[0]['summary_text']\n",
    "            else:\n",
    "                print(\"Unexpected response format:\", response_data)\n",
    "                return \"Error: Unexpected response format\"\n",
    "        except requests.exceptions.RequestException as e:\n",
    "            print(f\"Request failed (attempt {attempt + 1}): {e}\")\n",
    "            time.sleep(wait)  # Wait before retrying\n",
    "        except KeyError as e:\n",
    "            print(f\"KeyError: {e}, response data: {response_data}\")\n",
    "            return \"Error: Missing expected data\"\n",
    "    return \"Error: Failed after retries\"\n",
    "\n",
    "# Read emails from file\n",
    "with open(\"output.txt\", \"r\", encoding='latin-1') as file:\n",
    "    emails = file.read()\n",
    "\n",
    "# Function to split large text into smaller chunks\n",
    "def split_text(text, chunk_size=1024):\n",
    "    return [text[i:i + chunk_size] for i in range(0, len(text), chunk_size)]\n",
    "\n",
    "# Summarize each email\n",
    "individual_mails = []\n",
    "for email in emails.split('========================================================================='):\n",
    "    # Split email into smaller chunks if it's too large\n",
    "    chunks = split_text(email)\n",
    "    summary = \" \".join([summarize_text(chunk) for chunk in chunks])\n",
    "    individual_mails.append(summary)\n",
    "\n",
    "# Save summaries to a new file\n",
    "with open(\"summaries.txt\", \"a\") as file:\n",
    "    for summary in individual_mails:\n",
    "        file.write(summary + \"\\n\")\n",
    "\n",
    "print(\"Summaries saved to summaries.txt\")\n"
   ]
